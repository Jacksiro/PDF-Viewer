<Window x:Class="WpfPdfReader.GotoPageDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Goto page..." Height="100" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" WindowStyle="ToolWindow">
    <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBox x:Name="txtPage" Width="40"></TextBox>
        <Label Content="of" Width="30"/>
        <Label x:Name="lblMaxPageNumber" Width="50"></Label>
        <Button Content="Ok" Width="60" Click="Button_Click_1" Margin="0,0,4,0" IsDefault="True"></Button>
        <Button Content="Cancel" Width="60" Click="Button_Click_2" IsCancel="True"></Button>
    </StackPanel>
</Window>
